{
  "name": "color-generator",
  "version": "1.0.8",
  "description": "An awesome theme color generation scheme.",
  "license": "MIT",
  "homepage": "https://colors-docs.sh2.agoralab.co",
  "repository": "https://github.com/Johnson-hd/color-generator",
  "bugs": "https://github.com/Johnson-hd/color-generator/issues",
  "keywords": [
    "color",
    "colors",
    "generate",
    "hot"
  ],
  "authors": [
    "houdong"
  ],
  "main": "dist/color-generator.umd.js",
  "module": "dist/color-generator.es.js",
  "browser": "dist/color-generator.umd.js",
  "typings": "typings/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/color-generator.es.js",
      "require": "./dist/color-generator.umd.js"
    }
  },
  "bin": {
    "color-generator": "bin/color-generator.js"
  },
  "files": [
    "dist",
    "bin",
    "typings",
    "src",
    "examples",
    "templates"
  ],
  "scripts": {
    "start:example": "vite",
    "build": "tsc && vite build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --cache --fix"
    ],
    "*.css": [
      "prettier --parser=css --write"
    ]
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^16.11.6",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/runtime-core": "^3.2.22",
    "element-plus": "^1.1.0-beta.24",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "typescript": "^4.4.4",
    "unplugin-vue-components": "^0.16.0",
    "vite": "^2.6.12",
    "vue": "^3.2.16",
    "vue-tsc": "^0.28.9"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "color-generator": "^1.0.9",
    "commander": "^8.3.0",
    "inquirer": "^8.2.0",
    "tinycolor2": "^1.4.2"
  }
}
